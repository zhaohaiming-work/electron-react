{
  "name": "react-start-kit",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-package":"rimraf desk-package",
    "compile": "node build/script/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "start": "cross-env NODE_ENV=development node build/script/start",
    "ele:start": "cross-env NODE_ENV=development electron .",
    "win:build": "npm run clean-package && npm run build && electron-builder --win",
    "mac:build": "npm run clean-package && npm run build && electron-builder --mac",
    "lint": "eslint --ext .js src/",
    "lint:fix": "eslint --fix --ext .js --ext .jsx src/"
  },
  "keywords": [],
  "author": "haiming.zhao",
  "license": "ISC",
  "build": {
    "asar": {
      "smartUnpack": true
    },
    "productName": "桌面应用测试版本",
    "appId": "evaluate-platform",
    "copyright": " ",
    "directories": {
      "output": "desk-package"
    },
    "files": [
      "dist/**/*",
      "./main.js",
      "./project.config.js"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://ess.caicchina.com/desktopapp"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "artifactName": "package${version}.${ext}"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ],
      "artifactName": "package${version}.${ext}"
    },
    "linux": {
      "icon": "public/icons",
      "artifactName": "caic_setup_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": false
    }
  },
  "dependencies": {
    "antd": "^4.1.0",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.0",
    "core-js": "^2.6.5",
    "history": "^4.9.0",
    "mobx": "^4.8.0",
    "events": "^3.0.0",
    "mobx-react": "^6.1.1",
    "mobx-react-lite": "^1.4.1",
    "nprogress": "^0.2.0",
    "object-assign": "^4.1.1",
    "promise": "^7.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "whatwg-fetch": "^2.0.3",
    "ip": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "chalk": "^1.1.3",
    "compression": "^1.7.4",
    "cross-env": "^5.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "electron": "^8.2.2",
    "electron-builder": "^22.5.1",
    "express": "^4.14.0",
    "figures": "^2.0.0",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.1",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "http-proxy-middleware": "^0.19.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.5.3",
    "open": "^6.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "redbox-react": "^1.6.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.31.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0"
  }
}
